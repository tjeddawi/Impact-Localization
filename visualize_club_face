from Split_dataset import coords
import matplotlib.pyplot as plt

data = coords

x = [point[0] for point in data]
y = [point[1] for point in data]

plt.scatter(x, y, color='blue', marker='o')

plt.xlabel('X (mm)')
plt.ylabel('Y (mm)')
plt.title('Impact Locations')

plt.xlim(-35, 35)
plt.ylim(-20, 20)

plt.show()
